<html>
 <head>
  <meta http-equiv="Content-Type" content="text/html; charset=utf-8">
     <meta name="viewport" content="width=device-width">
    <TITLE>Рога и Копыта</TITLE>
    <link rel="stylesheet" href="flex.css">
    <link rel="stylesheet" href="wars.css">
    <link rel="stylesheet" href="style.css">
    <link rel="stylesheet" href="imgs.css">
    <link rel="stylesheet" href="animals.css">
 </head>
 <body>
 <div class='content'>
	<div class='centre'> 
		<div class='section' data-block='login_blk'> 
			<div class='line'> 
				<div class='desc'>Логин</div>
				<input data-id='login' data-save='abonent'  type=text   placeholder="Введите Логин" >		
			</div>
			<div class='line'> 
				<div class='desc'>Пароль</div>
				<input data-id='password'  type=password   placeholder="Введите Пароль" >		
			</div>
			<div class='line'> 
				<div class='desc'>Имя</div>
				<input data-id='name'   data-save='abonent' type=text   placeholder="Как к вам обращаться?" >		
			</div>
			<div class='line'> 
				<div class='btn'  data-action="recovery"   title = "Восстановление"></div>
				<div class='btn'  data-action="new_user"   title = "Регистрация"></div>		
			</div>
		</div>
		<div class='section' data-block='color'> 
			<div class='desc' data-write='code' >3215</div>
			<div class='desc' data-write='color_name' >синий</div>
		</div>
		<div class='table' data-table='bill'  data-block='bill'> </div>
		<div class='section' data-block='status'> 
			<div class='line'> 
				<div class='desc' data-write='status'>Вход</div>
				<div class='desc' data-write='time'>12:45:31</div>	
			</div>
		</div>
		<div class='section' data-block='buttons'> 
			<div class='line'>
			    <div class='btn' data-action="in_user" data-block='in_user' data-title='Вход' ></div>	
			    <div class='btn' data-action="check" data-block="check" data-title='Проверка' ></div>
			    <div class='btn' data-action="out_user"  data-block="out_user" data-title='Выход' ></div>
			    <div class='btn' data-action="story"  data-block="story"   data-title='История' ></div>	
			    <div class='btn' data-action="read"  data-block="read"  data-title='Состояние' ></div>
			    <div class='btn' data-action="bill"  data-block="bill"  data-title='Счет' ></div>
			    <div class='btn' data-action="login" data-block='login' data-title='Новый логин' ></div>	
			</div>
		</div>
		<div class='page_name'>Рога и Копыта</div>	
	</div>
	<div class='manual'>Справка</div>	
</div>

<script defer src="answer.js"></script>
<script defer src="arrs.js"></script>
<script defer src="commands.js"></script>
<script defer src="click.js"></script>
<script defer src="comm.js"></script>
<script defer src="library.js"></script>
<script defer src="control.js"></script>
<script defer src="start.js"></script>
 </body>
</html>